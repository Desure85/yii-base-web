{
    "name": "yiisoft/yii-app",
    "type": "library",
    "description": "Yii Framework Application",
    "keywords": ["yii", "framework", "application"],
    "homepage": "https://github.com/yiisoft/yii-app",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/github.com/yiisoft/yii-app/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "source": "https://github.com/yiisoft/yii-app"
    },
    "authors": [
        {
            "name": "Alexander Makarov",
            "email": "sam@rmcreative.ru"
        },
        {
            "name": "Paul Klimov",
            "email": "klimov.paul@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1.0",
        "yiisoft/yii-web": "^3.0@dev",
        "yiisoft/yii-console": "^3.0@dev",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/log": "^3.0@dev",
        "yiisoft/cache": "^3.0@dev",
        "yiisoft/yii-jquery": "^3.0@dev",
        "yiisoft/yii-captcha": "^3.0@dev",
        "yiisoft/yii-bootstrap4": "^3.0@dev",
        "yiisoft/yii-swiftmailer": "dev-master",
        "vlucas/phpdotenv": "^2.5",
        "hiqdev/composer-config-plugin": "dev-master"
    },
    "require-dev": {
        "yiisoft/yii-debug": "dev-master",
        "yiisoft/yii-gii": "dev-master",
        "phpunit/phpunit": "^7.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        },
        "config-plugin": {
            "params": "config/params.php",
            "common": "config/common.php",
            "web": "config/web.php"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {"yii\\app\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {"yii\\app\\tests\\": "tests"}
    },
    "config": {
        "process-timeout": 1800
    }
}
